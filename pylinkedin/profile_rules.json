{
    "company":{
       "url_placeholder":"https://www.linkedin.com/company/{}/",
       "extract_rules":{
          "com.linkedin.voyager.common.FollowingInfo":{
             "list":false,
             "fields":[
                "followerCount"
             ]
          },
          "com.linkedin.voyager.deco.organization.web.WebFullCompanyMain":{
             "list":false,
             "fields":[
                "staffCount",
                "companyEmployeesSearchPageUrl",
                "acquirerCompany",
                "phone",
                "entityUrn",
                "headquarter",
                "universalName",
                "companyPageUrl",
                "url",
                "jobSearchPageUrl",
                "staffingCompany",
                "staffCountRange",
                "school",
                "specialities",
                "confirmedLocations",
                "name",
                "fundingData",
                "description",
                "partnerCompanyUrl",
                "rankForTopCompanies",
                "topCompaniesListName",
                "paidCompany",
                "affiliatedCompanies",
                "foundedOn",
                "companyType",
                "groups"
             ],
             "custom_fields":{
                "id":{
                   "field":"entityUrn",
                   "regex":":(\\d*)$"
                }
             }
          },
          "com.linkedin.voyager.deco.organization.web.WebSimilarCompanyCardWithRelevanceReason":{
             "list":true,
             "fields":[
                "name",
                "url",
                "universalName",
                "school",
                "staffCountRange",
                "entityUrn"
             ]
          },
          "com.linkedin.voyager.deco.organization.web.WebCompanyStockQuote":{
             "list":false,
             "fields":[
                "stockQuotes"
             ]
          }
       },
       "subsections":{
          "employees":{
             "url_placeholder":"https://www.linkedin.com/search/results/people/?facetCurrentCompany=[\"{WebFullCompanyMain.id}\"]",
             "extract_rules":{
                "com.linkedin.voyager.identity.shared.MiniProfile":{
                   "list":true,
                   "fields":[
                      "firstName",
                      "lastName",
                      "occupation",
                      "objectUrn",
                      "publicIdentifier"
                   ]
                }
             },
             "crawler":{
                "prefix":"&page=",
                "limit":2
             }
          },
          "jobs":{
             "url_placeholder":"https://www.linkedin.com/jobs/search/?locationId=OTHERS.worldwide&f_C={WebFullCompanyMain.id}",
             "extract_rules":{
                "com.linkedin.voyager.deco.jserp.WebSearchJobJserpJobPostingLite":{
                   "list":true,
                   "fields":[
                      "title",
                      "formattedLocation",
                      "listedAt"
                   ]
                }
             },
             "crawler":{
                "prefix":"&start=",
                "limit":10
             }
          }
       }
    },
    "profil":{
       "url_placeholder":"https://www.linkedin.com/in/{}/",
       "extract_rules":{
          "com.linkedin.voyager.dash.deco.identity.profile.FullProfileWithEntities":{
             "list":false,
             "fields":[
                "name",
                "lastName",
                "firstName",
                "summary",
                "publicIdentifier",
                "volunteerCauses",
                "primaryLocale",
                "birthDateOn"
             ]
          },
          "com.linkedin.voyager.dash.deco.identity.profile.FullProfileSkill":{
             "list":true,
             "fields":[
                "name"
             ]
          },
          "com.linkedin.voyager.dash.deco.identity.profile.FullProfileEducation":{
             "list":true,
             "fields":[
                "degreeName",
                "schoolName",
                "fieldOfStudy",
                "dateRange",
                "entityUrn"
             ]
          },
          "com.linkedin.voyager.dash.deco.identity.profile.FullProfilePosition":{
             "list":true,
             "fields":[
                "title",
                "companyName",
                "companyUrn",
                "fieldOfStudy",
                "dateRange",
                "companyUrn",
                "entityUrn",
                "description",
                "geoLocationName",
                "locationName"
             ]
          }
       }
    }
 }